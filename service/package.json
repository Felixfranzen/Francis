{
  "name": "francis-service",
  "version": "0.0.1",
  "description": "Manage feature flags",
  "main": "index.js",
  "scripts": {
    "up": "docker-compose up -d && sleep 5",
    "down": "docker-compose down",
    "build": "tsc -p ./tsconfig.json",
    "start": "NODE_ENV=production node ./dist/src/index.js",
    "start:dev": "npm run up && npm run db:migrate:up:dev && NODE_ENV=development ts-node-dev --inspect=127.0.0.1:9234 ./src",
    "test": "npm run up && npm run db:migrate:up:dev && jest . --passWithNoTests && npm run down",
    "pgtyped:watch": "pgtyped -w -c pgtyped_config.json",
    "db:migrate:create": "db-migrate create -e pg",
    "db:migrate:up:dev": "db-migrate up -e dev",
    "db:migrate:down:dev": "db-migrate down -e dev",
    "db:migrate:up:prod": "db-migrate up",
    "db:migrate:down:prod": "db-migrate down"
  },
  "author": "Felix Franz√©n",
  "license": "MIT",
  "dependencies": {
    "@pgtyped/cli": "^0.9.0",
    "@pgtyped/query": "^0.9.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "pg": "^8.5.1",
    "swagger-ui-express": "^4.1.5",
    "typescript": "^4.1.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "prettier": "2.2.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0"
  }
}
